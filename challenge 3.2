class Student:
    def __init__(self, name, roll_number, cgpa):
        self.name = name
        self.roll_number = roll_number
        self.cgpa = cgpa

def sort_students(student_list):
    sorted_students = sorted(student_list, key=lambda x: x.cgpa, reverse=True)
    return sorted_students

# Create a list of 10 student objects for testing
students = [
    Student("Alice", "A123", 4.8),
    Student("Bob", "B456", 3.5),
    Student("Charlie", "C789", 3.9),
    Student("David", "D234", 3.2),
    Student("Eve", "E567", 3.7),
    Student("Frank", "F890", 3.6),
    Student("Grace", "G123", 4.0),
    Student("Hannah", "H456", 3.4),
    Student("Ivy", "I789", 3.8),
    Student("Jack", "J234", 3.3)
]

# Sort the list of students by CGPA
sorted_students = sort_students(students)

# Print the sorted list
for student in sorted_students:
    print(f"Name: {student.name}, Roll Number: {student.roll_number}, CGPA: {student.cgpa}")